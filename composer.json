{
  "name": "3ejoueur/overdog",
  "description": "A Craft CMS scaffolding project for 3e joueur",
  "keywords": [
    "craft",
    "cms",
    "3ejoueur",
    "Overdog"
  ],
  "require": {
    "craftcms/cms": "^3.4.0",
    "craftcms/redactor": "2.5.0",
    "solspace/craft-freeform": "3.6.2",
    "verbb/super-table": "2.4.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"copy('.env.example', '.env');\"",
      "@php -r \"unlink('composer.json');\"",
      "@php -r \"rename('composer.json.default', 'composer.json');\"",
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
