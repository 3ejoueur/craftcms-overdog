{
  "name": "3ejoueur/overdog",
  "description": "A Craft CMS scaffolding project for 3e joueur",
  "version": "1.1.2",
  "authors": [
    {
      "name": "Ian Reid Langevin",
      "email": "ian@3ejoueur.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "3ejoueur",
    "Overdog",
    "Craft CMS"
  ],
  "support": {
    "docs": "https://public-3e-joueur.gitlab.io/overdog-docs/"
  },
  "license": "MIT",
  "type": "project",
  "require": {
    "craftcms/aws-s3": "^1.2",
    "craftcms/cms": "3.5.14",
    "craftcms/redactor": "2.5.0",
    "verbb/super-table": "2.4.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
  "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"copy('.env.example', '.env');\"",
      "@php -r \"copy('README.example', 'README.md');\""
    ],
    "post-create-project-cmd": [
      "@php -r \"unlink('config/license.key');\"",
      "@php -r \"unlink('README.example');\"",
      "@composer dump-autoload -o"
    ],
    "post-install-cmd": [
      "@php -r \" define('HTSRC', 'web/.htaccess_' . getenv('ENVIRONMENT')); echo (file_exists(HTSRC) && copy(HTSRC, 'web/.htaccess')) ? HTSRC.' copied to web/.htaccess'.PHP_EOL : ''; \""
    ]
  }
}
