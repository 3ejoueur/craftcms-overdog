{
  "name": "overdog",
  "version": "1.2",
  "description": "Webpack and dev dependencies for Overdog",
  "author": "Ian Reid Langevin x 3e joueur",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack --config webpack.dev.js --watch",
    "build": "webpack --config webpack.prod.js --progress",
    "lint:scss": "yarn stylelint src/sass/**/*.scss",
    "lint:js": "yarn eslint src/js/**/*.js",
    "svg": "yarn rimraf src/svg/temp-optimized/* && node .imagemin-svgo.js && yarn svgstore -o web/img/svg-sprite.svg src/svg/temp-optimized/*.svg"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "imagemin": "^7.0.1",
    "imagemin-svgo": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss-webpack-plugin": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "svgstore-cli": "^2.0.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "flickity": "^2.2.2",
    "rfs": "^9.0.3"
  }
}
