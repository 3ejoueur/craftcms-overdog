{
  "name": "overdog",
  "version": "1.0.1",
  "description": "Webpack and dev dependencies for Overdog",
  "author": "Ian Reid Langevin x 3e joueur",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack --config webpack.dev.js --watch --hide-modules",
    "build": "webpack --config webpack.prod.js --progress --hide-modules",
    "lint": "yarn stylelint src/sass/**/*.scss",
    "lint:fix": "yarn stylelint src/sass/**/*.scss --fix",
    "svg": "yarn rimraf src/svg/temp-optimized/* && node .imagemin-svgo.js && yarn svgstore -o web/img/svg-sprite.svg src/svg/temp-optimized/*.svg --inline"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.0",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^4.2.0",
    "imagemin": "^7.0.1",
    "imagemin-svgo": "^8.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "flickity": "^2.1.2",
    "jquery": "^3.4.1",
    "lazysizes": "^5.2.0",
    "object-fit-images": "^3.2.4",
    "popper.js": "^1.16.1",
    "rimraf": "^3.0.2",
    "svg4everybody": "^2.1.9",
    "svgstore-cli": "^2.0.0"
  }
}
